/* pages/near/shop/shop.wxss */
page{
  position: relative;
  height: 100%
}
.page{
  font-size: 28rpx;
  color: #353535;
  background: #f4f4f4;
  box-sizing: border-box;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.font-blue{
  color: #59A1FF
}

/* 商家列表 */
.show-top{
  z-index: 11111;
  height: 238rpx;
  background: #fff;
  box-sizing: border-box;
  padding: 16rpx 0rpx 20rpx 0rpx;
}

.shop_info {
  display: flex;
  padding: 0 40rpx 0 20rpx;
}

.shop_logo {
  width: 140rpx;
  height: 130rpx;
  margin-right: 32rpx;
  border-radius: 8rpx;
}

.shop_r {
  flex: 1;
}

.shop_name {
  font-size: 36rpx;
  line-height: 52rpx;
  color: #353535;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.shop_activity {
  margin-top: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.shop_info_btn {
  width: 32rpx;
  height: 32rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.shop_info_btn image {
  width: 20rpx;
  height: 20rpx;
  display: block;
}

.shop_activity text {
  display: inline-block;
  padding: 0 16rpx;
  line-height: 40rpx;
  font-size: 20rpx;
  color: #59A1FF;
  border: 1px solid #59A1FF;
  border-radius: 4rpx;
  margin-right: 16rpx;
}

.search {
  display: flex;
  position: relative;
  align-items: center;
  padding: 0 40rpx 0 20rpx;
  box-sizing: border-box;
}

.search .securities_btn {
  width: 80rpx;
  height: 48rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 24rpx;
  color: #fff;
  position: relative;
}

.search .securities_btn image {
  width: 100%;
  height: 100%;
  display: block;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1;
}

.shop-more.show-info::after,.shop-more.show-info::before {
  display: none;
}

.shop-detail-wrap{
  font-size: 24rpx;
  color: #353535;
  background: #fff;
  padding: 32rpx 20rpx;
  line-height: 36rpx;
  width: 100%;
  box-sizing: border-box;
}

.shop-detail-wrap .item{
  display: flex;
  margin-top: 16rpx;
}

.shop-detail-wrap .item .time{
  color: #888;
  margin-left: 4rpx;
}

.shop-detail-wrap .item .label{
  width: 130rpx;
}
.shop-detail-wrap .item .content{
  flex: 1
}
.shop-more{
  width: 100%;
  background: #fff;
  display: flex;
  justify-content: center;
}

.shop-more::after{
  content: '';
  position: absolute;
  left: -40rpx;
  top: 20rpx;
  border-top: 1px solid #EDEDED;
  z-index: 11;
  width: 280rpx;
}

.shop-more::before{
  content: '';
  position: absolute;
  right: -40rpx;
  top: 20rpx;
  border-top: 1px solid #EDEDED;
  z-index: 11;
  width: 280rpx;
}

.shop-more .more-wrap{
  font-size: 22rpx;
  color: #999;
  background: #fff;
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.shop-more .icon{
  height: 20rpx;
  width: 20rpx;
  transform:rotate(90deg);
}
.shop-more .icon.reverse{
  transform:rotate(-90deg);
}

.shop_info_title {
  line-height: 40rpx;
  display: flex;
  align-items: center;
  margin-top: 32rpx;
}

.shop_info_title:first-child {
  margin-top: 0rpx;
}

.shop_info_title .title {
  font-size: 28rpx;
  font-weight: 700;
  margin-right: 8rpx;
}

.shop-locate{
  color: #999;
  font-size: 24rpx;
  line-height: 32rpx;
}
.shop-locate .icon{
  height: 32rpx;
  width: 32rpx;
  margin-right: 4rpx;
}

.show-top .close {
  margin-top: 60rpx;
  display: flex;
  justify-content: center;
  position: relative;
  left: 0;
  top: 0;
  width: 100%;
  height: 48rpx; 
}

.show-top .close image {
  width: 48rpx;
  height: 48rpx;
  display: block;
}

