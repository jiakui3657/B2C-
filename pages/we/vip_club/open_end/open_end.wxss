/* pages/we/vip_club/open_end/open_end.wxss */
.container  {
  padding: 0;
  text-align: center;
  background: #fff;
}

.logo {
  margin: 0 auto;
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
  background: #3B3B3B;
  padding: 40rpx;
  box-sizing: border-box;
  margin-top: 32rpx;
}

.logo image {
  width: 100%;
  height: 100%;
}

.title {
  height: 60rpx;
  line-height: 60rpx;
  margin-top: 32rpx;
  font-size: 40rpx;
  color: #009688;
}

.info {
  color: #353535;
  margin-top: 36rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 26rpx;
}

.check {
  width: 300rpx;
  text-align: center;
  height: 80rpx;
  line-height: 80rpx;
  margin: 0 auto;
  margin-top: 32rpx;
  font-size: 26rpx;
  color: #353535;
  border: 1px solid #353535;
}