/* pages/we/index/index.wxss */
/* 个人信息 */
.userInfo {
  height: 330rpx;
  overflow: hidden;
  position: relative;
  background: #f4f4f4
}

.userInfo_bj {
  width: 100%;
  height: 100%;
}

.user_info_t {
  display: flex;
}

.user_info_r {
  flex: 1;
}

.user_logo {
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
  display: block;
  border-radius: 50%;
}

.user_info_name {
  font-size: 32rpx;
}

.user_info_name text {
  font-size: 20rpx;
  margin-left: 8rpx;
}

.sign {
  height: 36rpx;
  /* line-height: 36rpx; */
  padding: 0 12rpx;
  border-radius: 36rpx;
  font-size: 22rpx;
  margin-top: 14rpx;
  background: #C63539;
  display: inline-block;
}

.sign image {
  height: 20rpx;
  width: 20rpx;
  margin-right: 6rpx;
  margin-top: 8rpx;
}

.share {
  position: absolute;
  right: 40rpx;
  top: 32rpx;
  text-align: center;
  font-size: 20rpx;
}

.share image {
  width: 32rpx;
  height: 32rpx;
}

.share view {
  line-height: 36rpx;
  height: 36rpx;
}

.user_info {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  padding: 24rpx 0 0 40rpx;
  box-sizing: border-box;
  color: #fff;
}

.user_vip {
  height: 70rpx;
  line-height: 70rpx;
  position: absolute;
  bottom: 0;
  left: 20rpx;
  width: 710rpx;
  box-sizing: border-box;
}

.user_vip_bj {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
}

.userInfo_vip {
  display: flex;
  justify-content: space-between;
  position: absolute;
  left: 0;
  top: 0;
  padding: 0 20rpx;
  box-sizing: border-box;
  width: 100%;
  color: #F9E69B;
  font-size: 28rpx;
}

.userInfo_vip_l image {
  width: 76rpx;
  height: 26rpx;
  margin-right: 10rpx;
}

.userInfo_vip_r {
  font-size: 22rpx;
}

.userInfo_vip_r image {
  width: 20rpx;
  height: 20rpx;
  margin-left: 6rpx;
}

.user_info_b {
  display: flex;
  padding: 30rpx 80rpx 30rpx 40rpx;
  justify-content: space-between;
  font-size: 26rpx;
  text-align: center;
}

.user_info_b .number {
  font-weight: 700;
  font-size: 28rpx;
}

.user_info_b view view {
  height: 40rpx;
  line-height: 40rpx;
}

/* 我的订单 */
.we_order {
  background: #fff;
  overflow: hidden;
  padding: 0 20rpx;
  padding-bottom: 32rpx;
}

.we_order_title {
  display: flex;
  justify-content: space-between;
  font-size: 32rpx;
  color: #101010;
  line-height: 104rpx;
  height: 104rpx;
}

.we_order_all {
  font-size: 22rpx;
  color: #888888;
}

.we_order_all text {
  vertical-align: middle;
}

.we_order_all image {
  width: 20rpx;
  height: 20rpx;
  vertical-align: middle;
}

.we_order_list {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

.we_order_list navigator {
  text-align: center;
  position: relative;
}

.we_order_list navigator .orderNum {
  position: absolute;
  top: 0;
  right: 0;
  width: 24rpx;
  height: 24rpx;
  font-size: 20rpx;
  color: #fff;
  background: #E51C23;
  border-radius: 50%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0;
}

.we_order_list image {
  width: 50rpx;
  height: 50rpx;
  display: block;
  margin: 0 auto;
}

.we_order_list text {
  color: #353535;
  font-size: 24rpx;
  display: block;
  margin-top: 16rpx;
}

/* 我的服务 */
.we_service {
  background: #fff;
  padding-bottom: 32rpx;
  overflow: hidden;
}

.we_service_title {
  margin-top: 32rpx;
  font-size: 32rpx;
  color: #101010;
  padding: 0 20rpx;
}

.we_service_list {
  display: flex;
  flex-wrap: wrap;
}

.we_service_list navigator {
  text-align: center;
  width: 25%;
}

.we_service_list view {
  text-align: center;
  width: 25%;
}

.we_service_list image {
  width: 60rpx;
  height: 60rpx;
  margin-top: 32rpx;
}

.we_service_list text {
  color: #353535;
  font-size: 24rpx;
  display: block;
  margin-top: 8rpx;
}

/* 邀请合伙人 */
.partner {
  padding: 0 20rpx;
  background: #fff;
  box-sizing: border-box;
  overflow: hidden;
}

.partner view {
  height: 104rpx;
  line-height: 104rpx;
  font-size: 32rpx;
  color: #101010;
}

.partner image {
  width: 100%;
  height: 146rpx;
  display: block;
  margin-bottom: 32rpx;
}

/* 积分兑换 */
.points {
  height: 160rpx;
}

.points image {
  width: 100%;
  height: 100%;
}

.goods_title {
  height: 48rpx;
  line-height: 48rpx;
  text-align: center;
  margin: 20rpx 0;
  color: #444444;
  font-weight: 900;
}

.goodsList {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}

/* 目标 */
.target {
  position: fixed;
  bottom: -160rpx;
  right: 20%;
  width: 20%;
  height: 160rpx;
  background: red;
}