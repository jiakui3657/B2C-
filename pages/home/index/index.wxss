/* pages/home/index/index.wxss */
page {
  background: #f8f8f8;
}

.container {
  background: #f8f8f8;
}

/* 搜索框 */
.search_box {
  height: 92rpx;
}

/* banner轮播图 */
.banner {
  height: 340rpx;
  position: relative;
  background-color: #F4F4F4;
}

.banner_bj {
  height: 170rpx;
  width: 100%;
  background: #fff;
}

.banner swiper {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
}

.banner swiper-item {
  padding: 0 16rpx 0rpx 16rpx;
  box-sizing: border-box;
  height: 100%;
}

.banner image {
  width: 100%;
  height: 100%;
  display: block;
  border-radius: 16rpx;
  /* box-shadow: 0px 0rpx 10rpx 10rpx rgba(0, 0, 0, .2); */
}

.banner_indicator {
  position: absolute;
  right: 36rpx;
  bottom: 20rpx;
  width: 60rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  border-radius: 32rpx;
  background: rgba(16, 16, 16, .2);
  font-size: 20rpx;
  color: #fff;
  z-index: 1;
}

/* 快捷入口 */
.quick {
  padding-bottom: 32rpx;
  height: 320rpx;
}

.quick swiper {
  height: 100%;
}

.quick swiper-item {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  align-items: center;
}

.quick navigator {
  text-align: center;
  width: 20%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.quick image {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
  margin-top: 32rpx;
}

.quick view {
  font-size: 26rpx;
  color: #353535;
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 8rpx;
}

/* 广告 */
.advertising {
  height: 160rpx;
  overflow: hidden;
}

.advertising image {
  height: 100%;
  width: 100%;
}

/* 秒杀 */
.seckill {
  background: #fff;
  padding: 0 20rpx;
}

.seckill_title {
  height: 112rpx;
  line-height: 112rpx;
  display: flex;
  justify-content: space-between;
}

.seckill_title_l {
  display: flex
}

.seckill_title_l text {
  font-size: 32rpx;
}

.countdown {
  margin-left: 16rpx;
  display: flex;
  align-items: center;
}

.countdown text {
  width: 34rpx;
  height: 34rpx;
  display: inline-block;
  border-radius: 4rpx;
  background: #373737;
  text-align: center;
  line-height: 34rpx;
  color: #fff;
  font-size: 22rpx;
  margin: 0 5rpx;
}

.seckill_title_r {
  font-size: 22rpx;
  color: #888888;
}

.seckill_title_r image {
  width: 16rpx;
  height: 16rpx;
}

.seckill_content {
  height: 400rpx;
  position: relative;
  -webkit-overflow-scrolling: touch;
}

.seckill_content swiper {
  height: 100%;
}

.seckill_content swiper-item {
  display: flex;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

.seckill_content .seckill_shop {
  width: 200rpx;
  position: relative;
  margin-right: 36rpx;
}

.seckill_content .seckill_shop:last-child {
  margin-right: 0rpx;
}

.seckill_content .seckill_shop_img  {
  width: 180rpx;
  height: 180rpx;
  text-align: center;
  position: relative;
  -webkit-overflow-scrolling: touch;
}

.seckill_content .seckill_shop_img image  {
  width: 180rpx;
  height: 180rpx;
}

.seckill_content .seckill_shop_img .shop_logo_animation  {
  width: 180rpx;
  height: 180rpx;
  position: absolute;
  left: 0;
  top: 0;
  overflow: auto;
  -webkit-overflow-scrolling: touch;
}

.seckill_content .seckill_shop_img .seckill_mask  {
  width: 100%;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 11111;
  background: rgba(0, 0, 0, .5);
  display: flex;
  justify-content: center;
  align-items: center;
}

.seckill_content .seckill_shop_img .seckill_mask image  {
  width: 120rpx;
  height: 120rpx;
  display: block;
}

.seckill_content .seckill_shop_name {
  font-size: 24rpx;
  color: #353535;
  min-height: 60rpx;
  /* line-height: 34rpx; */
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-top: 16rpx;
  overflow: hidden;
}

.seckill_content .seckill_shop_price {
  font-size: 24rpx;
  color: #FF474C;
  margin-top: 16rpx;
}

.seckill_content .seckill_shop_price image {
  width: 24rpx;
  height: 24rpx;
  vertical-align: middle;
}

.seckill_content .seckill_shop_original_price {
  font-size: 24rpx;
  color: #888888;
  text-decoration: line-through;
  margin-top: 8rpx;
}

.seckill_content .cart {
  width: 46rpx;
  height: 46rpx;
  position: absolute;
  right: 12rpx;
  bottom: 60rpx;
}

/* 轮播图自定义指示器 */
.seckill_indicator {
  position: absolute;
  bottom: 16rpx;
  width: 100%;
  display: flex;
  justify-content: center;
}

.seckill_indicator text {
  width: 10rpx;
  height: 10rpx;
  border-radius: 10rpx;
  background: #D2D2D2;
  margin-left: 12rpx;
}

.seckill_indicator .active {
  width: 20rpx;
  background: #FF474C;
}

/* 置顶板块 */
.plate {
  background: #fff;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  height: 520rpx;
}

.plate navigator {
  border-right: 1px solid #F4F4F4;
  border-bottom: 1px solid #F4F4F4;
  width: 50%;
  height: 50%;
  box-sizing: border-box;
}

.plate navigator image {
  width: 100%;
  height: 100%;
}

.plate navigator:nth-child(2) {
  border-right: 0px solid #F4F4F4;
}

.plate navigator:nth-child(3) {
  border-bottom: 0px solid #F4F4F4;
}

.plate navigator:nth-child(4) {
  border-right: 0px solid #F4F4F4;
  border-bottom: 0px solid #F4F4F4;
}

/* 会员广告 */
.vip {
  height: 160rpx;
  overflow: hidden;
}

.vip image {
  height: 100%;
  width: 100%;
}

/* 推荐商家 */
.recommend {
  background: #fff;
  padding: 0 20rpx;
  box-sizing: border-box;
}

.recommend .recommend_title {
  height: 112rpx;
  line-height: 112rpx;
  font-size: 32rpx;
  color: #353535;
  display: flex;
  justify-content: space-between;
}

.recommend .recommend_title_r {
  font-size: 22rpx;
  color: #999;
  display: flex;
  align-items: center;
}

.recommend .recommend_title image {
  width: 18rpx;
  height: 18rpx;
}

.recommend .recommend_title  text {
  font-size: 22rpx;
  color: #999999;
  display: inline-block;
  height: 34rpx;
  line-height: 34rpx;
}

.recommend_content {
  white-space: nowrap
}

.recommend_shop {
  width: 200rpx;
  margin-right: 32rpx;
  padding-bottom: 32rpx;
  display: inline-block;
}

.recommend_shop_img {
  width: 200rpx;
  height: 180rpx;
}

.recommend_shop_name {
  color: #353535;
  font-size: 28rpx;
  margin-top: 16rpx;
  height: 40rpx;
  line-height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}

.recommend_shop_code {
  font-size: 22rpx;
  color: #999;
  margin-top: 4rpx;
  display: flex;
  justify-content: space-between;
  height: 32rpx;
  line-height: 32rpx;
}

/* 商品分组 */
.goods_group {
  position: relative;
}

.box {
  /* height: 84rpx; */
  padding: 32rpx 0rpx 16rpx;
  display: flex;
  width: 100%;
  background: #f4f4f4;
  left: 0;
  z-index: 11;
  white-space: nowrap;
  -webkit-overflow-scrolling: touch;
  box-sizing: border-box;
}

.goods_group .goodsGroupList {
  margin-right: 24rpx;
  width: 120rpx;
  text-align: center;
  display: inline-block;
}

.goods_group .goodsGroupList:last-child {
  margin-right: 0rpx;
}

.goods_group text {
  font-size: 32rpx;
  display: inline-block;
  height: 48rpx;
  line-height: 48rpx;
}

.goods_group .goodsGroupList view {
  font-size: 24rpx;
  color: #888;
  width: 116rpx;
  text-align: center;
  height: 32rpx;
  line-height: 32rpx;
  margin-top: 4rpx;
  border-radius: 32rpx;
}

.goods_group .goodsGroupActive {
  color: #FF474C;
}

.goods_group .goodsGroupActive view {
  background: #FF474C;
  color: #fff;
}

.shop{
  background: #f4f4f4;
}

/* 购物车终点 */
.target {
  position: fixed;
  bottom: -160rpx;
  right: 20%;
  width: 20%;
  height: 160rpx;
  background: red;
}

/* 新客福利 */
.welfare {
  position: fixed;
  left: 50%;
  top: 50%;
  width: 580rpx;
  height: 620rpx;
  margin-left: -290rpx;
  margin-top: -310rpx;
  z-index: 1111;
}

.welfare image {
  width: 100%;
  height: 100%;
  display: block;
}

.welfare view {
  position: absolute;
  bottom: 62rpx;
  left: 0rpx;
  width: 100%;
  height: 100rpx;
  background: rgba(255, 255, 255, 0);
}

.welfare .price {
  position: absolute;
  bottom: 274rpx;
  left: 0rpx;
  display: flex;
  align-items: center;
  font-size: 96rpx;
  color: #E23754;
  padding-left: 68rpx;
}

.welfare .price text {
  font-size: 28rpx;
}

.welfare .welfare_del {
  position: absolute;
  left: 50%;
  margin-left: -24rpx;
  bottom: -78rpx;
  width: 48rpx;
  height: 48rpx;
}

/* 优惠券 */
.coupons {
  position: fixed;
  z-index: 111;
  left: 50%;
  top: 200rpx;
  background: #fff;
  width: 600rpx;
  height: 472rpx;
  border-radius: 10rpx;
  margin-left: -300rpx;
  padding: 48rpx 0rpx;
  box-sizing: border-box;
}

.coupons_info {
  height: 200rpx;
  width: 480rpx;
  margin: 0 auto;
  position: relative;
}

.coupons_info image {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  display: block;
  z-index: -1;
}

.coupons_note {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  font-size: 28rpx;
  color: #FF474C;
  display: flex;
  justify-content: center;
  align-items: center;
}

.coupons_info_l {
  display: flex;
  font-size: 96rpx;
  align-items: center;
  margin-right: 8rpx;
  font-weight: 700;
}

.coupons_info_l text {
  font-size: 36rpx;
  font-weight: 400;
}

.coupons_info_r {
  display: flex;
  flex-direction: column;
}

.coupons_info_r view {
  height: 40rpx;
  display: flex;
  align-items: center;
}

.coupons_info_r view:last-child {
  color: #888888;
}

.coupons_title {
  height: 40rpx;
  display: flex;
  align-items: center;
  margin: 32rpx 0;
  font-size: 28rpx;
  color: #353535;
  justify-content: center
}

.coupons button {
  width: 340rpx;
  height: 70rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #fff;
  background: #FF474C;
  font-size: 28rpx;
  padding: 0;
  margin: 0 auto;
}

.coupons_del {
  position: absolute;
  bottom: -108rpx;
  left: 50%;
  width: 48rpx;
  height: 48rpx;
  margin-left: -24rpx;
}