.seconds {
  position: relative;
  overflow: hidden;
}

.timeTable {
  display: flex;
  justify-content: space-between;
  height: 90rpx;
  background: #353535;
  color: #fff;
  font-size: 28rpx;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1111;
  
}

.timeList {
  width: 33.333%;
  text-align: center;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.timeList view:first-child {
  font-size: 32rpx;
  height: 48rpx;
  display: flex;
  align-items: center;
}

.timeList view:last-child {
  font-size: 24rpx;
  height: 36rpx;
  line-height: 36rpx;
}

.timeList:nth-child(1) view:first-child {
  display: flex;
  align-items: center;
}

.timeList:nth-child(1) view:first-child text {
