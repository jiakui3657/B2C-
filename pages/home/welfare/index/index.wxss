/* pages/home/welfare/welfare.wxss */
page {
  background: #f4f4f4;
  font-family: "Microsoft YaHei";
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
  height: 100%;
}

/*tab*/
.swiper-tab-list {
  width: 20%;
}

/*列表*/

/* 领劵中心 */
.securities {
  padding: 0 16rpx;
  overflow: hidden;
  position: relative;
}

/* banner轮播图 */
.banner {
  height: 320rpx;
  position: relative;
  background-color: #F4F4F4;
  margin-top: 16rpx;
  width: 100%;
  overflow: hidden;
}

.banner swiper {
  position: absolute;
  top: 0;
  width: 100%;
  height: 100%;
}

.banner swiper-item {
  box-sizing: border-box;
  height: 100%;
  padding: 0 16rpx;
}

.banner image {
  width: 100%;
  height: 100%;
  display: block;
  border-radius: 16rpx;
  /* box-shadow: 0px 0rpx 10rpx 10rpx rgba(0, 0, 0, .2); */
}

.banner_indicator {
  position: absolute;
  right: 36rpx;
  bottom: 20rpx;
  width: 60rpx;
  height: 32rpx;
  line-height: 32rpx;
  text-align: center;
  border-radius: 32rpx;
  background: rgba(16, 16, 16, .2);
  font-size: 20rpx;
  color: #fff;
  z-index: 1111;
}

.remind {
  height: 60rpx;
}

#remind {
  display: flex;
  align-items: center;
  height: 60rpx;
  background: #FFE8E9;
  color: #353535;
  font-size: 26rpx;
  box-sizing: border-box;
  width: 100%;
  z-index: 11;
  left: 0;
  top: 0;
  padding-left: 48rpx;
}

.remind text {
  color: #FF474C;
}


.securities .coupon_list {
  background: transparent;
  flex-direction: column;
}

.shop_list:first-child {
  margin-top: 0;
}

.coupon_info {
  flex: 1;
  display: flex;
  justify-content: space-between;
  position: relative;
}

.coupon_info_bj {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  z-index: -1;
}

.coupon_list_l {
  width: 240rpx;
  height: 220rpx;
  position: relative;
}

.coupon_list_l image {
  width: 80rpx;
  height: 28rpx;
  position: absolute;
  left: 0;
  top: 0;
}

.coupon_list_l_content {
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
  color: #D2D2D2;
  font-size: 24rpx;
  box-sizing: border-box;
  text-align: center;
}

.price {
  height: 100rpx;
  line-height: 100rpx;
  margin-top: 32rpx;
  font-size: 96rpx;
  font-weight: 700;
  display: flex;
  align-items: center;
  justify-content: center
}

.price text {
  font-size: 28rpx;
}

.condition {
  height: 40rpx;
  line-height: 40rpx;
  margin-top: 16rpx;
}

.securities .coupon_list_r {
  flex: 1;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  padding-left: 32rpx;
}

.coupon_list_t {
  flex: 1;
  padding: 24rpx 20rpx 32rpx 0;
  box-sizing: border-box;
  font-size: 28rpx;
  color: #353535;
  position: relative;
}

.coupon_list_t image {
  position: absolute;
  height: 100rpx;
  width: 120rpx;
  right: 0rpx;
  top: 0rpx;
}

.title {
  height: 40rpx;
  line-height: 40rpx;
  display: flex;
  align-items: center;
}

.title text {
  height: 32rpx;
  line-height: 32rpx;
  border-radius: 94rpx;
