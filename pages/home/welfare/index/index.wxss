  align-items: center;
  width: 16rpx;
  justify-content: center;
}

.timeList:nth-child(1) view:first-child .time {
  width: 32rpx;
  justify-content: center;
  height: 32rpx;
  border-radius: 4rpx;
  line-height: 30rpx;
  background: #fff;
  display: flex;
  color: #353535;
  font-size: 24rpx;
  align-items: center;
}

.activeTime {
  background: #FF474C;
}

